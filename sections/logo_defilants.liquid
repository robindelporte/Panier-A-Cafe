<div class="padding-section-medium">
  <div class="marquee_component">
    <div class="hide w-embed">
      <style>
        @keyframes scroll {
          from {
            transform: translateX(0);
          }
          to {
            transform: translateX(calc(-100% - 1rem));
          }
        }
        .scroll {
          animation: scroll 20s linear infinite;
        }
        .reverse {
          animation-direction: reverse;
        }
        .marquee-image {
          -webkit-transform: translateZ(0);
        }
      </style>
    </div>
    <h2 class="text-align-center">
      {{ section.settings.text_heading }}
    </h2>
    <div class="marquees_wrapper">
      {% for block in section.blocks %}
        {% if block.type == 'logo-defilants' %}
          <div li-block="Logo defilants" class="marquee">
            <div class="marquee-content scroll reverse">
              <img
                loading="eager"
                alt="{{ block.settings.image_logo_1.alt }}"
                src="{{ block.settings.image_logo_1 | img_url: 'master' }}"
                class="marquee-image"
              >
              <img
                loading="eager"
