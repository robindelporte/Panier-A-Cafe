</script><script>document.addEventListener('DOMContentLoaded', function() {
    const productRecommendationsSection = document.querySelector('.product-recommendations');
    const url = productRecommendationsSection.dataset.url;
    fetch(url)
      .then(response => response.text())
      .then(text => {
      const html = document.createElement('div');
      html.innerHTML = text;
      const recommendations = html.querySelector('.product-recommendations');
      if (recommendations && recommendations.innerHTML.trim().length) {
        productRecommendationsSection.innerHTML = recommendations.innerHTML;
      }
    })
      .catch(e => {
      console.error(e);
    });
  });
</script><script>document.addEventListener('DOMContentLoaded', function() {
    // S&eacute;lectionner tous les conteneurs de quantit&eacute;
    const quantityContainers = document.querySelectorAll('.quantity-container');
    // Pour chaque conteneur, initialiser ses propres contr&ocirc;les
    quantityContainers.forEach(function(container) {
        const decrementBtn = container.querySelector('.quantity-btn.decrement');
        const incrementBtn = container.querySelector('.quantity-btn.increment');
        const quantityDisplay = container.querySelector('.quantity-display');
        if (!decrementBtn || !incrementBtn || !quantityDisplay) {
            console.log('Un des &eacute;l&eacute;ments manque dans ce conteneur', container);
            return;
        }
        const minValue = parseInt(quantityDisplay.getAttribute('min')) || 1;
        const maxValue = parseInt(quantityDisplay.getAttribute('max')) || 99;
        // Update buttons state
        function updateButtonsState() {
            const currentValue = parseInt(quantityDisplay.value);
            decrementBtn.disabled = currentValue <= minValue;
            incrementBtn.disabled = currentValue >= maxValue;
        }
        // Increment quantity
        incrementBtn.addEventListener('click', function(e) {
            e.preventDefault(); // Emp&ecirc;cher la soumission du formulaire
            e.stopPropagation(); // Emp&ecirc;cher la propagation aux autres &eacute;l&eacute;ments
            const currentValue = parseInt(quantityDisplay.value);
            if (currentValue < maxValue) {
                quantityDisplay.value = currentValue + 1;
                updateButtonsState();
            }
        });
        // Decrement quantity
        decrementBtn.addEventListener('click', function(e) {
            e.preventDefault(); // Emp&ecirc;cher la soumission du formulaire
            e.stopPropagation(); // Emp&ecirc;cher la propagation aux autres &eacute;l&eacute;ments
            const currentValue = parseInt(quantityDisplay.value);
            if (currentValue > minValue) {
                quantityDisplay.value = currentValue - 1;
                updateButtonsState();
            }
        });
        // Emp&ecirc;cher le champ d'entr&eacute;e de soumettre le formulaire sur Entr&eacute;e
        quantityDisplay.addEventListener('keydown', function(e) {
            if (e.key === 'Enter') {
                e.preventDefault();
            }
        });
        // Initialize buttons state
        updateButtonsState();
        // Ajouter type="button" aux boutons de mani&egrave;re programmatique
        decrementBtn.setAttribute('type', 'button');
        incrementBtn.setAttribute('type', 'button');
    });
    // Ajouter un journal pour voir combien de conteneurs ont &eacute;t&eacute; initialis&eacute;s
    console.log('Nombre de conteneurs de quantit&eacute; initialis&eacute;s:', quantityContainers.length);
});
</script>
